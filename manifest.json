{
  "name": "My Cat Tab Extension",
  "version": "1.0.0",
  "description": "Make a good day with cat!",
  "manifest_version": 3,
  "author": {
    "name": "th33raphat",
    "email": "theeraphatchueanokkhum@gmail.com"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Simple extension"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "script/*.js",
        "images/*.jpg",
        "images/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "scripting"
  ]
}